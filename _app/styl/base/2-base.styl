*
	margin 0
	padding 0
	outline none
body
	font-size: 20px
	font-family: G
	color #000
	background-color #fff
	line-height: 1.2
	@media screen and (max-width 560px)
		font-size: 16px
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.wrapper
	overflow hidden
.content
	position relative
	z-index 60
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	margin 0
	text-decoration none
	color: #fff;
	width: auto
	text-transform uppercase
	font-size: 14px
	padding 20px 50px
	background: transparent
	border: 1px solid #fff
	transition: all .4s ease-in-out
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #000
		background-color #fff
.general-title
	margin 0 0 40px 70px
	display flex
	align-items: center
	justify-content: flex-start
	flex-wrap: nowrap
	flex-direction: row
	// &-white
	// 	color: #fff
	img
		margin-right: 30px
	h3
		font-size: 32px
		font-weight: bold
.item
	margin-bottom: 70px
	width 100%
	max-width 505px
	@media screen and (max-width 560px)
		margin-bottom: 40px
	&-2
		font-size: 32px
		@media screen and (max-width 560px)
			font-size: 16px
	&-blue
		color #113080
	&-img
		position relative
		top 0
		left -49px
		margin-bottom: 0
	&-title
		font-size: 32px
		font-weight: bold
		text-transform uppercase